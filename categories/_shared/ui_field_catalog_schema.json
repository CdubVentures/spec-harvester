{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://specfactory.local/schemas/ui_field_catalog_schema.json",
  "title": "UI Field Catalog",
  "type": "object",
  "required": [
    "category",
    "fields"
  ],
  "properties": {
    "category": {
      "type": "string",
      "minLength": 1
    },
    "generated_at": {
      "type": "string",
      "format": "date-time"
    },
    "fields": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "key"
        ],
        "properties": {
          "key": {
            "type": "string",
            "minLength": 1
          },
          "label": {
            "type": "string"
          },
          "group": {
            "type": "string"
          },
          "order": {
            "type": "number"
          },
          "required_level": {
            "type": "string"
          }
        },
        "additionalProperties": true
      }
    }
  },
  "additionalProperties": true
}
