{
  "uid": "sources_checklist",
  "title": "Sources Checklist",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Pages checked / min",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS"
      },
      "targets": [
        {
          "expr": "sum(rate(harvester_urls_checked_total[1m]))*60",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Blocked / min",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS"
      },
      "targets": [
        {
          "expr": "sum(rate(harvester_fetch_total{status_class=\"blocked\"}[1m]))*60",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Evidence used / min",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS"
      },
      "targets": [
        {
          "expr": "sum(rate(harvester_evidence_used_total[1m]))*60",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Fields covered (gauge)",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS"
      },
      "targets": [
        {
          "expr": "max(harvester_fields_covered_count)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    }
  ]
}