{
  "uid": "extraction_tab",
  "title": "Extraction Tab",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Extraction batches / min",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS"
      },
      "targets": [
        {
          "expr": "sum(rate(harvester_llm_call_total{role=\"extract\"}[1m]))*60",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Schema failures / min",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS"
      },
      "targets": [
        {
          "expr": "sum(rate(harvester_llm_schema_fail_total{role=\"extract\"}[1m]))*60",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Evidence policy violations / min",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS"
      },
      "targets": [
        {
          "expr": "sum(rate(harvester_evidence_policy_violation_total[1m]))*60",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Fields extracted / min",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS"
      },
      "targets": [
        {
          "expr": "sum(rate(harvester_fields_extracted_total[1m]))*60",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    }
  ]
}